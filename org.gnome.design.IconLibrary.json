{
    "app-id": "org.gnome.design.IconLibrary",
    "runtime": "org.gnome.Platform",
    "runtime-version": "3.38",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "icon-library",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--own-name=org.gnome.design.IconLibrary.SearchProvider"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin",
        "env": {
            "CARGO_HOME": "/run/build/icon-library/cargo"
        }
    },
    "modules": [{
            "name": "libhandy",
            "buildsystem": "meson",
            "config-opts": [
                "-Dintrospection=disabled",
                "-Dgtk_doc=false",
                "-Dtests=false",
                "-Dexamples=false",
                "-Dvapi=false",
                "-Dglade_catalog=disabled"
            ],
            "cleanup": [
                "/include",
                "/lib/pkgconfig",
                "/bin"
            ],
            "sources": [{
                "type": "git",
                "url": "https://gitlab.gnome.org/GNOME/libhandy.git",
                "branch": "1.2.0"
            }]
        },
        {
            "name": "icon-library",
            "buildsystem": "meson",
            "sources": [{
                "type": "archive",
                "url": "https://gitlab.gnome.org/World/design/icon-library/uploads/0f4ffcd3d378bf278d3599789f717a7e/icon-library-0.0.7.tar.xz",
                "sha256": "45f33957ed066012e32ec62a24e0a5d81b5c80c7d6293929b2431a5611c1031a"
            }]
        }
    ]
}
